{
    "global":{
        "env":"dev"
    },
    "exception_handler":{
        "class":"Telelab\\ExceptionHandler\\ExceptionHandler",
        "method":"catchException"
    },
    "error_handler":{
        "class":"Telelab\\ErrorHandler\\ErrorHandler",
        "method":"catchError"
    },
    "db":{
        "host":"localhost",
        "dbname":"*****",
        "user":"*****",
        "password":"*****"
    },
    "cache":{
        "enable":"1",
        "driver":"file",
        "file":{
            "ttl":"5",
            "path":"/data/Projects/www/telelab/var/cache/core"
        }
    },
    "logger":{
        "enable":"1",
        "driver":"file",
        "output":"1",
        "file":{
            "file":"/data/Projects/www/telelab/var/log/core.log"
        }
    },
    "generate":{
        "dir":"/data/Projects/www/telelab/var/data/",
        "alias":{
            "test:home:([a-z]+)":"index-$1.html"
        }
    },
    "route":{
        "test":{
            "type":"cli",
            "prefix":"/Test",
            "routing":"/data/Projects/www/telelab/app/Test/etc/routing.ini"
        },
        "test2":{
            "type":"cgi","prefix":"/test",
            "routing":"/data/Projects/www/telelab/app/Test/etc/routing.ini"
        }
    }
}